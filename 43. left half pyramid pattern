#include <stdio.h>
  
int main()
{
    int rows,i,j;
    printf("Enter the number of rows:\n");
    scanf("%d",&rows);
  
    for (i = 1; i < rows; i++) {                     
        for (j = 1; j < 2 * (rows - i) - 1; j++) {    
            printf(" ");
        }
        for (int k = 1; k <= i; k++) {            
            printf("%d ",k);
        }
        printf("\n");
    }
    return 0;
}
